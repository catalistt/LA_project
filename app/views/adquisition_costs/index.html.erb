<p id="notice"><%= notice %></p>

<h1>Adquisition Costs</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adquisition_costs.each do |adquisition_cost| %>
      <tr>
        <td><%= adquisition_cost.product_id %></td>
        <td><%= adquisition_cost.cost %></td>
        <td><%= link_to 'Show', adquisition_cost %></td>
        <td><%= link_to 'Edit', edit_adquisition_cost_path(adquisition_cost) %></td>
        <td><%= link_to 'Destroy', adquisition_cost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Adquisition Cost', new_adquisition_cost_path %>
