<p id="notice"><%= notice %></p>

<h1>Cash Registers</h1>

<table>
  <thead>
    <tr>
      <th>Initial cash</th>
      <th>Final cash</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cash_registers.each do |cash_register| %>
      <tr>
        <td><%= cash_register.initial_cash %></td>
        <td><%= cash_register.final_cash %></td>
        <td><%= link_to 'Show', cash_register %></td>
        <td><%= link_to 'Edit', edit_cash_register_path(cash_register) %></td>
        <td><%= link_to 'Destroy', cash_register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cash Register', new_cash_register_path %>
