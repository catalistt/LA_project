<% if @order.errors.any? %>
  Swal.fire({
    title: '¡Error!',
    text: 'Hubo un problema actualizando la orden',
    icon: 'error',
    confirmButtonText: '¡:C Ok!',
    timer: 3000
  });
  $("#delivery-info").html("<%= j render partial: 'delivery_info' %>");
<% else %>
  Swal.fire({
    title: '¡Logrado!',
    text: 'Se actualizaron las órdenes correctamente',
    icon: 'success',
    confirmButtonText: '¡Bacan!',
    timer: 3000
  });
  location.reload();
<% end %>